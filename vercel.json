{
    "name":"e-commerce",
    "version": 2,
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest":"/"
      }
    ],
    "rewriters": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ]
  }